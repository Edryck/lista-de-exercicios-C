3. *Valor da maça 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*VAlor da maca*// 
    int main() 
    { 
       int v; 
       printf("Quantas macas voce comprou?\n"); 
       scanf("%d",&v); 
       if (v>=12){ 
        printf("Cada maca custa R$1.00"); 
       } else if (v<12){ 
            printf("Cada maca custa R$1.30"); 
           }  
       return 0; 
    } 
