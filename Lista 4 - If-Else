4. Aprovado ou reprovado 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Aprovado ou reprovado*// 
    int main() 
    { 
       float n1,n2,m; 
       printf("Digite a nota 1:\n"); 
       scanf("%f",&n1); 
       printf("Digite a nota 2:\n"); 
       scanf("%f",&n2); 
       m=(n1+n2)/2; 
       if (m>=6){ 
        printf("Aprovado"); 
       }else{ 
       printf("Reprovado"); 
       } 
       return 0; 
    } 
    -----------------------------------------------------
5. Pode ou nâo votar 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Pode ou nâo votar*// 
    int main() 
    { 
       int a,n,m; 
       printf("Digite em que ano estamos:\n"); 
       scanf("%d",&a); 
       printf("Digite o ano em que voce nasceu:\n"); 
       scanf("%d",&n); 
       m=a-n; 
       if(m<16){ 
        printf("Nao pode votar"); 
        }else if (m>=16){ 
            printf("Pode votar"); 
        } 
  
       return 0; 
    } 
    -----------------------------------------------------
6. Maior e menor que 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Maior e menor que*// 
    int main() 
    { 
       float a,b; 
       printf("Digite o valor 1:\n"); 
       scanf("%f",&a); 
       printf("Digite o valor 2:\n"); 
       scanf("%f",&b); 
       if(a<b){ 
        printf("%.2f eh menor que %.2f",a,b); 
        }else if (a>b){ 
            printf("%.2f eh maior que %.2f",a,b); 
        } 
  
       return 0; 
    } 
    -----------------------------------------------------
7. Escrever em ordem crescente 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Escrever em ordem crescente*// 
    int main() 
    { 
       float a,b; 
       printf("Digite o valor 1:\n"); 
       scanf("%f",&a); 
       printf("Digite o valor 2:\n"); 
       scanf("%f",&b); 
       if(a<b){ 
        printf("%.2f ; %.2f",a,b); 
        }else if (a>b){ 
            printf("%.2f ; %.2f",b,a); 
        } 
  
       return 0; 
    } 
