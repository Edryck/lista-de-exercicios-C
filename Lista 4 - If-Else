1. É ou não maior que 10 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*É ou não maior que 10*// 
    int main() 
    { 
       int v; 
       printf("Digite o valor que deseja conferir:\n"); 
       scanf("%d",&v); 
       if (v>10){ 
        printf("Eh maior que 10"); 
       } else if (v<10){ 
            printf("Eh menor que 10"); 
           } else if (v=10){ 
                printf("Eh 10"); 
            } 
       return 0; 
    } 
    -----------------------------------------------------
2. Positivo ou negativo 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Positivo ou negativo*// 
    int main() 
    { 
       int v; 
       printf("Digite o valor que deseja conferir:\n"); 
       scanf("%d",&v); 
       if (v>0){ 
        printf("Eh positivo"); 
       } else if (v<0){ 
            printf("Eh negativo"); 
           } 
       return 0; 
    } 
