8.Hora extra 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Extra*// 
    int main() 
    { 
       float a,b,h,e,c; 
       printf("Digite o total do banco de horas desse mes:\n"); 
       scanf("%f",&a); 
       printf("Digite o valor do salario:\n"); 
       scanf("%f",&b); 
       h=b/40; 
       e=h*1.5; 
       if (a>160){ 
        c=a-160; 
        c=e+b; 
        printf("Seu salario eh: R$%.2f",c); 
        }else if (a<=160){ 
           printf("Seu sslario eh: R$%.2f",b); 
       } 
       return 0; 
    } 
    -----------------------------------------------------
9.Extra 2 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Extra 2*// 
    int main() 
    { 
       float s,v,t,m,c; 
       printf("Digite o valor do seu salario:\n"); 
       scanf("%f",&s); 
       printf("Digite o valor de vendas efetuadas:\n"); 
       scanf("%f",&v); 
       c=v*.03; 
       m=v*.05; 
       if (v<=1500){ 
            t=c+s; 
            printf("O valor total do seu salario eh: R$%.2f",t); 
       }else if (v>1500){ 
            t=m+s; 
            printf("O valor total do seu salario eh: R$%.2f",t); 
       } 
       return 0; 
    } 
