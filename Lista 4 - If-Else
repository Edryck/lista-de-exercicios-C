10. Saldo bancário 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Saldo bancario*// 
    int main() 
    { 
       int n; 
       float s,d,c,a; 
       printf("Digite o numero da sua conta:\n"); 
       scanf("%d",&n); 
       printf("Digite o seu saldo:\nR$"); 
       scanf("%f",&s); 
       printf("Digite quantidade debitada:\nR$"); 
       scanf("%f",&d); 
       printf("Digite quantidade creditada:\nR$"); 
       scanf("%f",&c); 
       a=s-d+c; 
       printf("Seu saldo atual eh de R$%.2f\n",a); 
       if (a>=0){ 
        printf("O seu saldo esta positivo"); 
       }else { 
         printf("O seu saldo esta negativo"); 
         } 
       return 0; 
    } 
    -----------------------------------------------------
11. Efetuar compra de acordo com a quantidade em estoque 
    #include <stdlib.h> 
    //*Efetuar compra de acordo com a quantidade em estoque*// 
    int main() 
    { 
       int qe,mx,mn; 
       float m; 
       printf("Digite a quantidade do produto em estoque:\n"); 
       scanf("%d",&qe); 
       printf("Digite a quantidade maxima que pode deixar em estoque:\n"); 
       scanf("%d",&mx); 
       printf("Digite a quantidade minima que pode deixar em estoque:\n"); 
       scanf("%d",&mn); 
       m=(mx+mn)/2; 
       printf("A quantidade media eh: %.1f\n",m); 
       if (qe>=m){ 
        printf("Nao efetuar compra"); 
       } else { 
          printf("Efetuar compra"); 
          } 
       return 0; 
    } 
    -----------------------------------------------------
12. Valor é positivo, negativo ou zero 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Valor é positivo, negativo ou zero*// 
    int main() 
    { 
       float n; 
       printf("Digite um numero:\n"); 
       scanf("%f",&n); 
       if (n<=-1){ 
        printf("Esse numero eh negativo"); 
       }else if (n>=1){ 
         printf("Esse numero eh positivo"); 
         } else printf("Esse numero eh zero"); 
       return 0; 
    } 
    -----------------------------------------------------
13. Combustível 
#include <stdio.h> 
#include <stdlib.h> 
  
//*Combustivel*// 
  
int main() { 
    float q, v; 
    char t; 
    float A=2.9,G = 3.3; 
    printf("Quantidade de litros vendidos:\n"); 
    scanf("%f",&q); 
    while ((getchar())!='\n'); 
    printf("Digite o tipo de combustivel (A para alcool; G para gasolina): "); 
    scanf("%c",&t); 
    if (t=='A'){ 
        if (q<=20){ 
            v=(A-A*.03)*q; 
        } else { 
            v=(A-A*.05)*q; 
        } 
    } else if (t=='G'){ 
        if (q<=20){ 
            v=(G-G*.04)*q; 
        } else { 
            v=(G-G*.06)*q; 
        } 
    } else { 
        printf("Caractere invalido.\n"); 
        return 1; 
    } 
    printf("O valor a ser pago eh de: R$%.2f\n",v); 
    return 0; 
    } 
    -----------------------------------------------------
14. 
#include <stdio.h> 
#include <stdlib.h> 
int main() 
{ 
    int h1,h2,m1,m2,hn,hv,mn,mv,s,p; 
    printf("Digite a idade do primeiro homem:\n"); 
    scanf("%d",&h1); 
    printf("Digite a idade do segundo homem:\n"); 
    scanf("%d",&h2); 
    printf("Digite a idade da primeira mulher:\n"); 
    scanf("%d",&m1); 
    printf("Digite a idade da segunda mulher:\n"); 
    scanf("%d",&m2); 
     if (h1>h2) { 
        hv=h1; 
        hn=h2; 
    } else { 
        hv=h2; 
        hn=h1; 
    } 
    if (m1>m2) { 
        mv=m1; 
        mn=m2; 
    } else { 
        mv=m2; 
        mn=m1; 
    } 
    s=hv+mn; 
    p=hn*mv; 
    printf("Soma das idades do homem mais velho com a mulher mais nova eh: %d\n",s); 
    printf("Produto das idades do homem mais novo com a mulher mais velha eh: %d\n",p); 
    return 0; 
} 
    -----------------------------------------------------
15.Fruteira 
    #include <stdio.h> 
    #include <stdlib.h> 
//*Compras*// 
    int main() 
    { 
    float kgmo,kgma,pmo,pma,t,vt; 
    float d=0.0,pmo5=2.50,pmoa5=2.20,pma5=1.80,pmaa5=1.50; 
    printf("Digite a quantidade de morangos (em Kg):\n"); 
    scanf("%f",&kgmo); 
    printf("Digite a quantidade de macas (em Kg):\n"); 
    scanf("%f",&kgma); 
    if (kgmo>5){ 
        pmo=kgmo*pmoa5; 
    } else{ 
        pmo= kgmo*pmo5; 
    } 
    if (kgma>5){ 
        pma=kgma*pmaa5; 
    }else{ 
        pma=kgma*pma5; 
    } 
    vt=pmo+pma; 
    if ((kgmo+kgma>8)||(vt>25)){ 
        d=vt*0.10; 
    } 
    t=vt-d; 
    printf("O valor a ser pago eh: R$%.2f\n",t); 
        return 0; 
    } 
    -----------------------------------------------------
16. Preço do produto e desconto 
    #include <stdio.h> 
    #include <stdlib.h> 
    //*Preço do produto e desconto*// 
    int main() 
    { 
        int q; 
        float v,vt,d,p; 
        char n[20]; 
        printf("Digite o nome do produto: "); 
        scanf("%s",n); 
        printf("Digite a quantidade do produto: "); 
        scanf("%d",&q); 
        printf("Digite o preco unitario do produto: R$"); 
        scanf("%f",&p); 
        v=q*p; 
        if (q<=5){ 
            (d=0.02); 
        }else if ((q>5)&&(q<=10)){ 
            (d=0.03); 
        }else if (q>10){ 
            (d=0.05); 
        } 
        vt=v*(1-d); 
        printf("O valor eh: R$%.2f\nO valor total com desconto eh: R$%.2f",v,vt); 
        return 0; 
    }
